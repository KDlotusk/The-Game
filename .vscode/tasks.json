{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Générer le projet",
      "type": "shell",
      "windows": {
        "command": "qmake || make"
      },
      "linux": {
        "command": "qmake  ; make"
      },
      "problemMatcher": [],
      "options": {
        "cwd": "${workspaceFolder}${pathSeparator}build"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
